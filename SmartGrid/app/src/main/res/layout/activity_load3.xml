                        android:layout_height="70dp"
                        android:layout_below="@+id/textView2"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="20dp"
                        app:srcCompat="@drawable/voltage" />
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
        </LinearLayout>

