                        android:layout_height="70dp"
                        android:layout_below="@+id/textView4"
                        android:layout_centerHorizontal="true"
                        android:layout_marginTop="20dp"
                        app:srcCompat="@drawable/power" />
                </RelativeLayout>
            </androidx.cardview.widget.CardView>
